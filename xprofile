# setxkbmap -model abnt2 -layout br -variant abnt2 # keyboard layout
wal -R # Load pywal colors and wallpaper
xrdb -load ~/.Xresources # Load Xresources

/usr/lib/kdeconnectd & # Start KDEconnect
picom & # Start compositor
parcellite -n & # clipboard manager
udiskie & # mount helper

# discord # Start discord
